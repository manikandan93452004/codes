import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        String s1=s.nextLine();
        String s2=s.nextLine();
        int a=s.nextInt();
        StringBuffer sb=new StringBuffer(s1);
        sb.insert(a,s2);
        System.out.println(sb);
    }
}
